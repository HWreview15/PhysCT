        <!--<img src="https://via.placeholder.com/728x90?text=728x90+Leaderboard">-->
        <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-7596539695738361" crossorigin="anonymous"></script>
        <!-- footer -->
        <ins class="adsbygoogle"
             style="display:inline-block;width:728px;height:90px"
             data-ad-client="ca-pub-7596539695738361"
             data-ad-slot="7640792773"></ins>
        <script>
             (adsbygoogle = window.adsbygoogle || []).push({});
        </script>

      </div>
    </footer>
    <!-- BootstrapJS -->
    <script src="/Plugin/jquery-3.4.1.min.js"></script>
    <script src="/Plugin/popper.min.js"></script>
    <script src="/Plugin/bootstrap-4.4.1.js"></script>
  </body>
</html>
